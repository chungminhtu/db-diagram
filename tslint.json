{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "linterOptions": {
        "exclude": [
            "src/@gen/**"
        ]
    },
    "jsRules": {
        "max-line-length": {
            "options": [
                120
            ]
        }
    },
    "rules": {
        "interface-name": false
    },
    "rulesDirectory": []
}